{
    "configurations": [
        {
            "name": "avr-gcc-linux",
            "includePath": [
                "${workspaceFolder}/firmware/avrlib/inc",
                "${workspaceFolder}/firmware/inc"
            ],
            "defines": [
                "AVR",
                "__AVR_ATmega328P__"
            ],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/avr-gcc",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}